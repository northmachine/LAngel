<div id="state" class="content-section-b" style="border-top: 0">
    <style>
        input{
            border: 0px;
            outline:none;
            cursor: pointer;
            text-align:center;
        }
    </style>
    <div class="container">

        <div class="col-md-6 col-md-offset-3 text-center wrap_title">
            <h2>状态</h2>
            <p class="lead" style="margin-top:0">实时健康</p>
        </div>

            <div class="row">
                <div class="col-sm-4 wow fadeInDown text-center">
                    <img class="rotate" src="img/icon/heart.svg" alt="Generic placeholder image">
                    <h3>心率</h3>
                    <input id="rate" class="lead" value="78/min" name="rate" readonly>
                </div><!-- /.col-lg-4 -->

                <div class="col-sm-4 wow fadeInDown text-center">
                    <img  class="rotate" src="img/icon/map.svg" alt="Generic placeholder image">
                    <h3>步数</h3>
                    <input id="step" class="lead" value="7890" name="step" readonly>
                </div><!-- /.col-lg-4 -->

                <div class="col-sm-4 wow fadeInDown text-center">
                    <img  class="rotate" src="img/icon/clock.png" alt="Generic placeholder image" >
                    <h3>清醒时间</h3>
                    <input id="awake" class="lead" value="8.5Hour" name = "awake" readonly>
                </div><!-- /.col-lg-4 -->
            </div><!-- /.row -->

            <div class="row tworow">
                <div class="col-sm-4  wow fadeInDown text-center">
                    <img class="rotate" src="img/icon/sleep2.png" alt="Generic placeholder image">
                    <h3>深度睡眠</h3>
                    <input id="deep" class="lead" value="3Hour" name="deep" readonly>
                </div><!-- /.col-lg-4 -->

                <div class="col-sm-4 wow fadeInDown text-center">
                    <input type="image" name="name" value ="submit" class="rotate" src="img/icon/upload.png" alt="Generic placeholder image" onclick="upload()">
                    <h3>上传数据</h3>
                </div><!-- /.col-lg-4 -->

                <div class="col-sm-4 wow fadeInDown text-center">
                    <img  class="rotate" src="img/icon/sleep1.png" alt="Generic placeholder image">
                    <h3>浅度睡眠</h3>
                    <input id="shallow" class="lead" value="5.5Hour" name="shallow" readonly>
                </div><!-- /.col-lg-4 -->
            </div><!-- /.row -->

    </div>
</div>

